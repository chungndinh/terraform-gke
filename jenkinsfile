pipeline {

  agent any

  environment {
    GOOGLE_APPLICATION_CREDENTIALS = credentials('key-terraform')
  }
  stages {
    stage('TF Plan') {
      steps {
	  sh 'echo GOOGLE_APPLICATION_CREDENTIALS'
          sh 'ls'
          sh 'terraform init'
          sh 'terraform plan -out myplan'
        
      }      
    }
   }

}

